{
    "parser": "babel-eslint",
    "rules": {
        "indent": [
            2,
            4
        ],
        "no-alert": 0,
        "consistent-return": 0,
        "max-len": 0,
        "import/first": 0,
        "no-mixed-operators": 0,
        "class-methods-use-this": 0,
        "no-template-curly-in-string": 0,
        "no-underscore-dangle": 0,
        "prefer-spread": 0,
        "no-plusplus": 0,
        "no-restricted-syntax": 0,
        "no-prototype-builtins": 0,
        "no-useless-escape": 0,
        "global-require": 0,
        "newline-per-chained-call": 0,
        "arrow-parens": 0,
        "prefer-destructuring": 0,
        "import/no-named-as-default": 0,
        "import/no-named-as-default-member": 0,
        "import/prefer-default-export": "off",
        "no-var": "off",
        "no-undef": "off",
        "no-multi-str": "off",
        "vars-on-top": "off",
        "no-tabs": "error",
        "func-names": "off",
        "no-unused-expressions": "off",
        "prefer-rest-params": "off",
        "no-unused-vars": "off",
        "no-multi-assign": "off",
        "no-mixed-spaces-and-tabs": 2,
        "one-var": 0,
        "no-shadow": 0,
        "prefer-const": 0,
        "default-case": 0,
        "no-new": 0,
        "no-use-before-define": 0,
        "no-loop-func": 0,
        "no-useless-constructor": 0,
        "no-bitwise": 0,
        "block-scoped-var": 0,
        "no-inner-declarations": 0,
        "guard-for-in": 0,
        "no-redeclare": 0,
        "radix": 0,
        "no-useless-concat": 0,
        "no-restricted-globals": 0,
        "no-array-constructor": 0,
        "no-param-reassign": 0,
        "new-cap": 0,
        "no-empty-function": 0,
        "no-cond-assign": 0,
        "no-constant-condition": 0,
        "camelcase": 0
    },
    "globals": {
        "$": true,
        "jQuery": true
    },
    "env": {
        "es6": true,
        "browser": true
    },
    "extends": "airbnb/base"
}
